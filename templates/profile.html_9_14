          <span ng-click="profileView(ProfileInfo.users_type)"  ng-if="to_view_type != 'view'" >
          <img src="public/images/edit2.png"></span>
        </div>
        <div style="clear: both;"></div>
        <div  id="p_id" ng-show="display_tab" style="font-size:24px;"  align="center">
